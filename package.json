{
  "name": "single-spa-vue",
  "version": "1.0.0",
  "description": " 基于single-spa的vue微前端项目",
  "main": "index.js",
  "scripts": {
    "install:all": "cnpm i",
    "start": "concurrently \"npm run start:app\" \"npm run start:goods\" \"npm run start:customers\" \"npm run start:main\" \"npm run start:iframe\"",
    "start:goods": "cd goods && npm run serve",
    "start:customers": "cd customers && npm run serve",
    "start:app": "cd app && npm run serve",
    "start:main": "cd main && npm run serve",
    "start:iframe": "cd iframe && npm run serve",
    "build": "concurrently \"npm run build:app\" \"npm run build:goods\" \"npm run build:customers\" \"npm run build:main\"",
    "build:app": "cd app && npm run build",
    "build:goods": "cd goods && npm run build",
    "build:customers": "cd customers && npm run build",
    "build:main": "cd main && npm run build",
    "build:iframe": "cd iframe && npm run build"
  },
  "author": "fanwenqiang",
  "license": "ISC",
  "homepage": "https://github.com/QxQstar/single-spa-vue#readme",
  "dependencies": {
    "ant-design-vue": "^1.7.2",
    "axios": "^0.19.2",
    "core-js": "^2.6.5",
    "hytools": "^1.2.0",
    "@hydesign/grape": "^1.2.0",
    "single-spa": "^4.4.1",
    "single-spa-vue": "^1.5.2",
    "systemjs-webpack-interop": "^1.1.0",
    "vue": "^2.6.10",
    "jsencrypt": "^3.0.0-rc.1",
    "vue-router": "^3.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-plugin-unit-jest": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.0.0",
    "cross-env": "^6.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "less": "^3.0.4",
    "less-loader": "^5.0.0",
    "postcss-plugin-namespace": "^0.0.2",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "serve": "^11.2.0",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "vue-cli-plugin-single-spa": "^1.0.1",
    "vue-property-decorator": "^9.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  }
}
